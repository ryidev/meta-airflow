# API Configuration
# IMPORTANT: Update API_BASE_URL with your ngrok URL for physical device testing
API_BASE_URL=http://localhost:3000/api/v1/m
API_TIMEOUT=15000

# App Configuration
APP_NAME=RentVerse
APP_VERSION=1.0.0

# Google OAuth - Web Client ID (WAJIB untuk backend verification)
GOOGLE_WEB_CLIENT_ID=your-google-web-client-id

# Google OAuth - Android Client ID (opsional, hanya untuk referensi)
GOOGLE_ANDROID_CLIENT_ID=your-google-android-client-id

# Feature Flags
ENABLE_OAUTH=true
ENABLE_AI_PREDICTION=false
ENABLE_PUSH_NOTIFICATIONS=false

# Pagination
DEFAULT_PAGE_SIZE=10

# Upload Limits
MAX_IMAGE_SIZE=5242880
MAX_IMAGES_PER_PROPERTY=5
